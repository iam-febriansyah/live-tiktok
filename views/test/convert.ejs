<html>
    <head>
    </head>
    <body> 
        <input type="file" id="file" onchange="onchangeFile()" style="display: none;" >
        <br><br>
        <img for="file" style="width: 100; height: 100; cursor: pointer;" id='a' src='data:image/png;base64,'/>
        <br>
        <input type="text" id="filename" style="border: none;" value="No file choosen">
        <br>
        <button onclick="save('jpeg')" id="jpeg">JPEG</button>
        <button onclick="save('jpg')" id="jpg">JPG</button>
        <button onclick="save('png')" id="png">PNG</button>
        <button onclick="save('webp')" id="webp">WEBP</button>
        <script>
            var imgBtn = document.querySelector(`#a`);
            imgBtn.addEventListener('click', function() {
                var fileInp = document.querySelector('[type="file"]');
                fileInp.click();
            })

            async function onchangeFile(){
                var fileNameTxt = document.getElementById('filename');
                const file = document.querySelector(`#file`);
                file.disabled = true;
                fileNameTxt.value = "Loading..";
                const originalImage = document.querySelector(`#a`);
                originalImage.src = await fileToDataUri(file.files[0]);
                file.disabled = false;
                file.innerHTML = "No file choosen";
                var theSplit = file.value.split('\\');
                var fileName = theSplit[theSplit.length-1];
                fileNameTxt.value = fileName;
            }
            function fileToDataUri(field) {
                return new Promise((resolve) => {
                    const reader = new FileReader();
                    reader.addEventListener("load", () => {
                        resolve(reader.result);
                    });
                    reader.readAsDataURL(field);
                });
            }
            async function webpToJpg(id, ext) {
                var image = new Image();
                image.crossOrigin = 'anonymous';
                image.onload = function () {
                    var canvas = document.createElement('canvas');
                    canvas.width = this.naturalWidth;
                    canvas.height = this.naturalHeight;
                    canvas.getContext('2d').drawImage(this, 0, 0);
                    document.getElementById(id).src = canvas.toDataURL('image/'+ext);
                };
                image.src = document.getElementById(id).src;
            }
            function sleep(ms) {
                return new Promise(resolve => setTimeout(resolve, ms));
            }
            async function save(ext){
                var fileNameTxt = document.getElementById('filename').value;
                var btn = document.getElementById(ext);
                btn.innerHTML = 'Loading..';
                btn.disabled = true;
                await webpToJpg('a', ext);
                await sleep(1000);
                var img = document.getElementById('a').src;
                var a = document.createElement("a");
                a.href = img
                a.download = ext.toUpperCase()+"_"+fileNameTxt+"."+ext; 
                a.click();
                btn.innerHTML = ext.toUpperCase();
                btn.disabled = false;
            }
        </script>
    </body>
</html>
